{
  "version": 2,
  "builds": [
    {
      "src": "main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.js"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production",
      "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
    }
  },
  "functions": {
    "main.js": {
      "memory": 3008,
      "maxDuration": 120
    }
  }
}
